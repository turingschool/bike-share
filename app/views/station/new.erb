<h2>New Station</p>

<form action = "/stations" method="post">
  <label>Name:
    <input type="text" name="station[name]"/>
  </label><br/>

  <label>Dock Count:
    <input type="text" name="station[dock_count]"/>
  </label><br/>

  <label>City:
    <input type="text" name="station[city]"/>
  </label><br/>

  <label>Installation Date:
    <input type="date" name="station[installation_date]"/>
  </label><br/>

  <label>Latitude:
    <input disabled name="station[lat]"/>
  </label><br/>

  <label>Longitude:
    <input disabled name="station[long]"/>
  </label><br/>

  <input type="submit"/>
</form>
