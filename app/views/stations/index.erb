<h1>Stations</h1>


<% @stations.each do |station| %>
  <ul> Name: <%= station.name %>
  <ti> Number of Docks: <%= station.dock_count %> </ti>
  <ti> City: <%= station.city.name %> </ti>
  <ti> Install Date: <%= station.install_date %> </ti>
  </ul>
  <a href='/stations/edit/<%=station.id%>'>Edit Station</a>
  <form action='/stations/delete/<%=station.id%>' method="POST" >
    <input type='hidden' name='_method' value="DELETE" />
    <input type='submit' value='Delete' />
  </form>
<%end%>

<br><a href='/stations/new'>Add Station</a>