<h1> New Station </h1>

<form action"/stations" method="POST">
  <h3>Create a new station</h3>
  <input type= "text" name="station[name]" value="Name"/><br>
  <input type= "text" name="station[dock_count]" value="Dock count"/><br>
  <input type= "text" name="station[city]" value="City"/><br>
  <input type= "text" name="station[installation_date]" value="Installation Date"/><br><br>
  <input type= "submit" />
</form>
