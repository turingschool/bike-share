<h2>Create a new trip</h2>
<form action="/trips" method="post">
  <input type="text" name="trip[duration]"           value = "Enter the Duration"><br/>
  <input type="text" name="trip[start_date]"         value = "Enter the Starting Date"><br/>
  <input type="text" name="trip[start_station]"      value = "Enter the Starting Station"><br/>
  <input type="text" name="trip[end_date]"           value = "Enter the Ending Date"><br/>
  <input type="text" name="trip[end_station]"        value = "Enter the Ending Station"><br/>
  <input type="text" name="trip[bike_id]"            value = "Enter the Bike ID"><br/>
  <input type="text" name="trip[subscription_type]"  value = "Enter the Subscription Type"><br/>
  <input type="text" name="trip[zip_code]"           value = "Enter the Zip Code"><br/>

  <input type="submit" value="Create Trip"><br/>
</form>
