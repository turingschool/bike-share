    <table class="table">
      <thead>
        <tr>
          <th>Trip ID</th>
          <th>Duration</th>
          <th>Start Date</th>
          <th>Start Station ID</th>
          <th>End Date</th>
          <th>End Station Id</th>
          <th>Bike ID</th>
          <th>Subscription Type</th>
          <th>Zip Code</th>
          <th>Delete</th>
          <th>Update</th>
        </tr>
      </thead>

      <tbody>
        <!-- <% count = 0 %> -->
        <th scope="row" id="total entries"></th>
        <% @trips.each do |trip| %>
        <tr>
          <td><a href="trips/<%= trip.id %>"><%= trip.id %></a></td>
          <td><%= trip.duration%></td>
          <td><%= trip.start_date%></td>
          <td><%= trip.start_station_id%></td>
          <td><%= trip.end_date%></td>
          <td><%= trip.end_station_id%></td>
          <td><%= trip.bike_id%></td>
          <td><%= trip.subscription_type%></td>
          <td><%= trip.zip_code%></td>
          <td><form class="btn-form" id="trip_<%= trip.id %>" action="/trips/<%=trip.id%>" method="POST">
            <input class="button btn btn-delete" type="hidden" name="_method" value="DELETE">
            <input id="delete_trip" type="submit" value="Delete"/>
          </form></td>
          <td><a id="Update_<%= trip.id %>" type="button" class="btn btn-info" href="/trips/<%=trip.id%>/edit">Update<a/></td>
        </tr>
        <% end %>
      </tbody>
    </table>
  <%= will_paginate @trips %>
