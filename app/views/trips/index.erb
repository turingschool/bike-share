  <div class="row">

    <div class="col-md-16">
      <hr>
      <table class="table table-striped">
        <thead>
          <tr><th>Duration</th><th>Start Date</th><th>Start Station</th><th>End Date</th><th>End Station</th><th>Bike ID</th><th>Subscription Type</th><th>Zipcode</th><th>&nbsp;</th></tr>
        </thead>
        <tbody>
          <% @trips.each do |trip| %>
          <tr>
            <td><%= trip.duration %></td>
            <td><%= trip.start_date %></td>
            <td><%= trip.start_station.try(:name) %></td>
            <td><%= trip.end_date %></td>
            <td><%= trip.end_station.try(:name) %></td>
            <td><%= trip.bike_id %></td>
            <td><%= trip.subscription.try(:subscription) %></td>
            <td><%= trip.zipcode.try(:zip_code) %></td>
            <td><a href="/trips/<%= trip.id %>/edit"><button type="button" class="btn btn-info">Edit Trip</button></a>
						<td><form action="/trips/<%= trip.id %>" method="post">
              <input type="hidden" name="_method" value="DELETE" />
