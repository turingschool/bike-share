<h1> Trip <%= @trip.id  %> </h1>
  <p>Duration: <%= @trip.duration%></p>
  <p>Start Date: <%= @trip.start_date%></p>
  <p>Start Station ID: <%= @trip.start_station_id%></p>
  <p>End Date: <%= @trip.end_date%></p>
  <p>End Station ID: <%= @trip.end_station_id%></p>
  <p>Bike ID: <%= @trip.bike_id%></p>
  <p>Subscription Type ID: <%= @trip.subscription_id%></p>
  <p>Zip Code: <%= @trip.zip_code%></p>
  <form action="/trips/<%= @trip.id %>" method="post">
    <input type="hidden" name="_method" value="DELETE">
    <input type="submit" value="DELETE THIS TRIP">
  </form>
  <form action="/trips/<%= @trip.id %>/edit" method="put">
    <input type="hidden" name="_method" value="PUT">
    <input type="submit" value="Edit This Trip">
  </form>
<a href="/trips">View all Trips</a>
