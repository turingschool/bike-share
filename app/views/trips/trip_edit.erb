<form id="edit" action="/trips/<%= @trip.id %>/edit" method="post">
<h1>Update a trip</h1>
<input type="hidden" name="_method" value="PUT" />
<p>duration:<input type='text' name='duration' value="<%= @trip.duration %>"></p><br>
<p>trip date: <input type='text' name='trip_date' value="<%= @trip.trip_date %>"></p><br>
<p>start station: <input type='text' name='start_station' value="<%=@trip.start_station%>"></p><br>
<p>end station: <input type='text' name='end_station' value="<%=@trip.end_station%>"></p><br>
<p>bike id: <input type='text' name='bike_id' value="<%=@trip.bike_id%>"></p><br>
<p>subscription type: <input type='text' name='subscription_type' value="<%=@trip.subscription_type.subscription_type%>"></p>
<p>zip code: <input type='text' name='zip_code' value="<%= @trip.zip_code.zip_code %>"</p>

<!-- <button type="button">

 </button> -->

<input id='thing' type='submit'/>
</form>
